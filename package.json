{
    "name": "rawb-react-ssr",
    "version": "2.0.6",
    "description": "Frontend Service Side Render for React and RAWB",
    "main": "dist/index.js",
    "directories": {
        "lib": "dist"
    },
    "scripts": {
        "dev": "./node_modules/@babel/cli/bin/babel.js . --ignore dist,dev,node_modules --out-dir ./dist --source-maps inline --verbose --watch ",
        "build": "./node_modules/@babel/cli/bin/babel.js . --ignore dist,dev,node_modules --out-dir ./dist",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/24hr-malmo/rawb-react-ssr.git"
    },
    "keywords": [
        "react",
        "ssr",
        "rawb"
    ],
    "author": "Camilo Tapia <camilo.tapia@24hr.se>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/24hr-malmo/rawb-react-ssr/issues"
    },
    "homepage": "https://github.com/24hr-malmo/rawb-react-ssr#readme",
    "eslintConfig": {
        "env": {
            "browser": true,
            "commonjs": true,
            "es6": true,
            "node": true,
            "mocha": true
        },
        "parser": "babel-eslint",
        "extends": "eslint:recommended",
        "parserOptions": {
            "ecmaFeatures": {
                "experimentalObjectRestSpread": true
            },
            "sourceType": "module",
            "ecmaVersion": "2017"
        },
        "plugins": [],
        "rules": {
            "indent": [
                "error",
                4,
                {
                    "SwitchCase": 1
                }
            ],
            "linebreak-style": [
                "error",
                "unix"
            ],
            "quotes": [
                "error",
                "single"
            ],
            "semi": [
                "error",
                "always"
            ],
            "camelcase": 1
        }
    },
    "dependencies": {
        "lru-cache": "4.1.3",
        "he": "1.2.0"
    },
    "peerDependencies": {
        "redux": ">=3",
        "redux-thunk": ">=2",
        "react-redux": ">=5",
        "react": ">=16",
        "react-helmet": ">=5",
        "react-router-dom": ">=4",
        "react-dom": ">=16",
        "react-loadable": ">=5"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.4",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-proposal-export-default-from": "^7.2.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
        "@babel/preset-env": "^7.4.4",
        "@babel/preset-react": "^7.0.0"
    }
}
